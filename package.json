{
  "name": "followswyp",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "npm run update:accounts && npm run build && npm run export && firebase deploy",
    "update": "tsc --p ./api && node ./api/update",
    "update:all": "npm run update -- game accounts && npm run format:data",
    "update:game": "npm run update -- game && npm run format:data",
    "update:accounts": "npm run update -- accounts && npm run format:data",
    "format": "prettier --write '**/*.{ts,tsx,json}'",
    "format:data": "prettier --write '**/data/**/*.json'",
    "format:verify": "prettier --list-different '**/*.{ts,tsx,json}'"
  },
  "author": "Erik Schierboom",
  "license": "MIT",
  "dependencies": {
    "milligram": "^1.3.0",
    "next": "^9.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
